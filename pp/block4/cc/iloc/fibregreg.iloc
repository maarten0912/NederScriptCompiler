loadAI r_arp,@a => r_a          // load 'a'
loadI 2 => r_2                  // constant 2 into r_2
loadAI r_arp,@b => r_b          // load 'b'

mult r_a,r_2 => r_a             // r_a <- a * 2
mult r_a,r_b => r_b             // r_b <- (a * 2) * b
add  r_a,r_b => r_a             // r_a <- (a * 2) + (a * 2 * b)
