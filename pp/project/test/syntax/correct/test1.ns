functie hoofd() {
    publiek Reeks<Getal> lijst = [1,2,3];

    draad {
        lijst[0] = 2;
    }

    aansluiten();

    afdrukken("De som van de lijst is ");
    Getal som = 0;
    voor (Getal i = 0; i < lengte(lijst); i = i + 1) {
        som = som + lijst[i];
    }
    afdrukken(som);
    afdrukken("\n");
}